{
    "parser": "Babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        // "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "max-len": "off"
    },
    "plugins":  ["jest"],
    "extends": ["eslint:recommended", "google"]
}
