{
    "parser": "Babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        // "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "max-len": "off",
        "indent": "off"
        
    },
    "extends": ["eslint:recommended", "google"]
}